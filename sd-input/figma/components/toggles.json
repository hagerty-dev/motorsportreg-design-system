          },
          "off": {
            "value": "{color.transparency.00}",
            "type": "color"
          }
        }
      },
      "border-radius": {
        "value": "{border.radius.full}",
        "type": "borderRadius"
      }
    },
    "color": {
      "background": {
        "value": "{color.gray.300}",
        "type": "color"
      }
    },
    "border-radius": {
      "value": "{border.radius.full}",
      "type": "borderRadius"
    }
